{
  "name": "planning-app",
  "version": "1.0.0",
  "description": "Planning App con integrazione Google Calendar e Drive",
  "private": true,
  "scripts": {
    "install:all": "npm install --prefix server && npm install --prefix client",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "postinstall": "npm run install:all"
  },
  "keywords": [
    "planning",
    "productivity",
    "calendar",
    "google-calendar",
    "google-drive"
  ],
  "author": "Francesco Daddi <francesc.daddi@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
